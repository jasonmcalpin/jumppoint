{
    "extends": "airbnb-base",
    "rules": {
      "func-names": [
        "error",
        "never"
      ],
      "max-len": [
        1,
        120,
        2,
        {
          "ignoreComments": true
        }
      ],
      "quote-props": [
        1,
        "consistent-as-needed"
      ],
      "no-cond-assign": [
        2,
        "except-parens"
      ],
      "space-infix-ops": 0,
      "no-unused-vars": [
        1,
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "default-case": 0,
      "no-param-reassign": 0,
      "quotes": 0
    }
  }